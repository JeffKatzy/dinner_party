<h1>Choose your menu!</h1>
<%= form_for [@dinner, @menu_item] do |f| %>
  <ul>
    <% @dishes.each do |dish| %>
      <li>
      <input type="checkbox" name="menu_item[dish_ids][]" value="<%= dish.id%>"> 
      <%= f.label dish.name %></li>
    <% end %>
  </ul>
  <p>
  <%= f.label :other %>
  <%= f.text_field :other %> 
  <p>
  <%= f.submit "Create Menu"%>
<% end %>